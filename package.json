{
  "name": "yprobe",
  "version": "1.0.0",
  "description": "A quick Kubernetes YAML manifest sanity checker for identifying security issues",
  "author": "Kubernetes Security Team <k8s-security@example.com>",
  "homepage": "/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/k8s-security/yprobe.git"
  },
  "keywords": [
    "kubernetes",
    "yaml",
    "security",
    "k8s",
    "manifest",
    "scanner"
  ],
  "dependencies": {
    "diff": "^5.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9"
  }
}
